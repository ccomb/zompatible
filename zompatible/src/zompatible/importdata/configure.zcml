        permission="zope.ManageContent"
        set_schema=".interfaces.IImportData"
    />
</zope:class>

<zope:utility
      component=".importdata.importDataFactory"
      name="zompatible.importData"
      permission="zope.Public"
/>


<include package=".browser" />
<include package=".doc" />



